  <delete id="delete_layer" parameterClass="com.focus3d.pano.model.ibator.PanoPerspectiveLayerModel">
	  DELETE
	    layer.*,
	    element.*,
	    elementProduct.* 
	  FROM
	    pano_perspective_layer layer
	    LEFT JOIN pano_perspective_element element
			ON element.`LAYER_SN` = layer.sn
	    LEFT JOIN pano_perspective_element_product elementProduct
	      ON element.sn = elementProduct.`ELEMENT_SN`
	  WHERE	layer.sn = #sn#
  </delete>
  
