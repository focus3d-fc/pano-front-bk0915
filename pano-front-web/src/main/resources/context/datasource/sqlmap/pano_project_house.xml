<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap namespace="pano_project_house">

	<typeAlias alias="housestype" type="com.focus3d.pano.model.pano_project_house" />


	<select id="getHousetype" resultClass="housestype"
		parameterClass="Long">
		select
		SN,`NAME`,IMG_SN
		from pano_project_house
		where
		PROJECT_SN =
		#PROJECT_SN#
	</select>

	<delete id="delHousetypebySN" parameterClass="Long">
		delete from
		pano_project_house
		where SN = #SN#
	</delete>

	<select id="selHousetypebySN" resultClass="housestype"
		parameterClass="Long">
		select
		SN,ID
		from pano_project_house
		where
		SN
		=#SN#
	</select>

	<insert id="addHousetype" parameterClass="housestype">
		insert into
		pano_project_house
		(`NAME`,IMG_SN,ADD_TIME,PROJECT_SN)
		values(#NAME#,#IMG_SN#,#ADD_TIME#,#PROJECT_SN#)
	</insert>

	<update id="upHousetype" parameterClass="housestype">
		update
		pano_project_house
		<dynamic prepend="SET">
			<isNotEmpty prepend="AND" property="NAME">
				pano_project_house.`NAME`=#NAME#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="IMG_SN">
				pano_project_house.IMG_SN=#IMG_SN#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="UPDATE_TIME">
				pano_project_house.UPDATE_TIME=#UPDATE_TIME#
			</isNotEmpty>
		</dynamic>
		where SN = #SN#
	</update>

	<select id="getHousetypebySN" resultClass="housestype"
		parameterClass="Long">
		select
		SN,`NAME`
		from pano_project_house
		where
		SN
		=#SN#
	</select>

</sqlMap>